{
  "name": "@yankeeinlondon/link-builder",
  "version": "1.0.0-beta.7",
  "description": "adds contextual classes and features to links in documents used in vite-plugin-md",
  "keywords": [
    "vitejs",
    "builder",
    "vite-plugin-md",
    "vue"
  ],
  "homepage": "https://github.com/yankeeinlondon/link-builder#readme",
  "bugs": {
    "url": "https://github.com/yankeeinlondon/link-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yankeeinlondon/link-builder.git"
  },
  "license": "MIT",
  "author": "Ken Snyder <ken@ken.net>",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts --clean",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:test": "eslint test/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest:ui",
    "watch": "tsup src/index.ts --format=esm,cjs --dts --watch",
    "audit:fix": "pnpm audit --fix",
    "release": "run-s lint release:latest test:ci audit:fix release:bump",
    "release:latest": "pnpm install",
    "release:bump": "bumpp"
  },
  "dependencies": {
    "vite-plugin-md": "*",
    "pathe": ">=0.3.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.17.2",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/ui": "^0.18.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "esno": "^0.16.3",
    "fp-ts": "^2.12.1",
    "happy-dom": "^6.0.3",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prismjs": "^1.28.0",
    "ts-node": "^10.8.1",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.18.1",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "markdown-it": ">=13"
  }
}
